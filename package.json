  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsc && tsc-alias",
    "start": "node --optimize_for_size dist/server.js",
    "dev": "nodemon --config nodemon.json --exec ts-node src/server.ts",
